<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GestionEstudiantesAdmin">

    <Button
        android:id="@+id/GestionEstudiantes_crearEstudiantes"
        android:layout_width="349dp"
        android:layout_height="80dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="24dp"
        android:onClick="irACrearEstudiante"
        android:text="Crear estudiantes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/gestionEstudiantes_consultarEstudiantes"
        android:layout_width="349dp"
        android:layout_height="80dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="24dp"
        android:onClick="irAConsultarEstudiantes"
        android:text="consultar estudiantes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/GestionEstudiantes_crearEstudiantes" />
</androidx.constraintlayout.widget.ConstraintLayout>