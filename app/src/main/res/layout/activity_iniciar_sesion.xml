<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IniciarSesion">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="19dp"
        android:text="Iniciar Sesion"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/IniciarSesion_btVolver"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/IniciarSesion_btVolver"
        android:layout_width="88dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="19dp"
        android:text="Volver"
        android:textSize="12sp"
        app:iconTint="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="54dp"
        android:text="Nombre de usuario"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IniciarSesion_btVolver" />

    <EditText
        android:id="@+id/IniciarSesion_EtNombreUsuario"
        android:layout_width="382dp"
        android:layout_height="60dp"
        android:layout_marginStart="18dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="Contraseña"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IniciarSesion_EtNombreUsuario" />

    <EditText
        android:id="@+id/IniciarSesion_ETContraseña"
        android:layout_width="377dp"
        android:layout_height="53dp"
        android:layout_marginStart="14dp"
        android:ems="10"
        android:inputType="numberPassword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/IniciarSesion_BtIniciarSesion"
        android:layout_width="188dp"
        android:layout_height="82dp"
        android:layout_marginStart="111dp"
        android:layout_marginTop="291dp"
        android:text="Iniciar Sesion"
        app:iconTint="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IniciarSesion_ETContraseña" />

</androidx.constraintlayout.widget.ConstraintLayout>