<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Gestion_DocentesAdmin">

    <Button
        android:id="@+id/gestionDocentesAdmin_btCrearDocente"
        android:layout_width="343dp"
        android:layout_height="77dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="30dp"
        android:onClick="irACrearDocente"
        android:text="Crear Docente"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/gestionDocentesAdmin_btCrearDocente2"
        android:layout_width="343dp"
        android:layout_height="77dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="30dp"
        android:onClick="irAConsultarUser"
        android:text="Consultar Docentes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gestionDocentesAdmin_btCrearDocente" />
</androidx.constraintlayout.widget.ConstraintLayout>